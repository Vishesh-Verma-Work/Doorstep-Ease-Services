<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Service Details</title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
</head>
<link rel="stylesheet" href="/service.css">
</head>

<body>

    <h2>
        <%= serviceName %>
    </h2>

    <!-- serviceName : title,
    desc : description,
    type : upperDropdown,
    cost : price, 
    name : name,
    exp : Experience,
    add : address,
    email : email,
    code1 : primaryCountryCode,
    num1 : primaryPhoneNumber,
    code2 : alternateCountryCode,
    num2 : alternatePhoneNumber, -->

    <div class="tar">
        <div id="carouselExampleIndicators" class="carousel slide">

            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="<%= img1 %>" class="b-block w-100" alt="...">
                </div>
                <div class="carousel-item">
                    <img src="<%= img2 %>" class="b-block w-100" alt="...">
                </div>

            </div>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators"
                data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>
    </div>




    <div class="data">
        <% for(data of mainData) { %>
            <div class="sec">
                <div class="l">
                    <div class="name">
                        <%= data.serviceName %>
                    </div>
                    <div class="des dess">
                        <%= data.desc %> <br>
                    </div>

                    <div class="des rup">
                        <span>&#8377;</span>
                        <%=data.cost %> 
                        <span id="only">only</span>
                    </div>

                    <div class="des">
                        <h5>Primary Number :</h5>
                        <%= data.code1 %> &nbsp;&nbsp; <%= data.num1 %>
                    </div>

                    
                



                        <div class="des">
                            <h5>Service Provider Name : </h5>
                            <%= data.name %> <br>
                        </div>


                        <div class="des">
                            <h5>Service Experience : </h5>
                            <%= data.exp %> <br>
                        </div>

                       

                        <div class="des">
                            <h5>Email : </h5>
                            <%= data.email %> <br>
                        </div>



                        <div class="des">
                            <h5>Secondary Number : </h5>
                            <%= data.code2 %> &nbsp;&nbsp; <%= data.num2 %>
                        </div>
                   
                        <div class="des">
                            <h5>Address : </h5>
                            <%= data.add %> <br>
                        </div>
                    
                    
                </div>


                <div class="img">
                    <img src="<%= unk %>" alt="">
                </div>

            </div>

            <% } %>




    </div>

   
    <a href="/home"><button id="space">Back</button></a>


    <script src="/service.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
        crossorigin="anonymous"></script>

        

</body>

</html>